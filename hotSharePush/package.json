{
  "name": "hotSharePush",
  "version": "1.1.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "build": "coffee --output . --compile .",
    "install": "npm install",
    "start": "browserify -t coffeeify inspector.js > bundle.js && electron .",
    "startmaster": "browserify -t coffeeify inspector.js > bundle.js && node main_cluster.js",
    "startslaver": "browserify -t coffeeify inspector.js > bundle.js && isClient=1 node main_cluster.js"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "body-parser": "~1.0.1",
    "cluster": "~0.7.7",
    "co": "^4.6.0",
    "coffeeify": "^2.0.1",
    "express": "~4.14.0",
    "geoip-lite": "1.1.8",
    "kue": "~0.11.1",
    "mongodb": "~2.1.18",
    "mongoid-js": "~1.0.6",
    "socket.io": "^1.4.8",
    "underscore": "~1.8.3",
    "url": "0.11.0",
    "apn": "1.6.2",
    "jpush-sdk": "3.1.0",
    "mqtt":"^2.7.2",
    "mongo-oplog": "1.0.1"
  },
  "devDependencies": {
    "browserify": "~13.0.1"
  }
}
